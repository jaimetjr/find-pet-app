{
  "expo": {
    "name": "Achando um Lar",
    "slug": "find-pet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/golden_icon.png",
    "scheme": "findpetapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaimetjr.findpetapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show pets near you and help you find them a home."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jaimetjr.findpetapp",
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./path/to/google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "notification": {
      "icon": "./src/assets/trayIcon2.png",
      "color": "#FF4500",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} novas mensagens"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
        "expo-router",
        "expo-font",
        "expo-web-browser",
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show pets near you.",
            "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location in the background.",
            "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show pets near you."
          }
        ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4572a145-1d00-4330-96fb-c04337461325"
      },
      "clerkRedirectUrl": "findpetapp://oauth-callback"
    }
  }
}
